{
    "version": 2,
    "builds": [
        {
            "src": "/src/index.js",
            "use": "@vercel/node"
        },
        {
            "src": "./uploads/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/uploads/(.*)",
            "dest": "/uploads/$1"
        },
        {
            "src": "/api/(auth|posts|users)/(.*)",
            "dest": "/src/index.js"
        }
    ]
}